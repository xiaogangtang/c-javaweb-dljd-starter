<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>asrvlet</servlet-name>
        <servlet-class>com.apexsoft.servlet.AServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>asrvlet</servlet-name>
        <url-pattern>/a</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>bsrvlet</servlet-name>
        <servlet-class>com.apexsoft.servlet.BServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bsrvlet</servlet-name>
        <url-pattern>/b</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>tset</servlet-name>
        <servlet-class>com.apexsoft.servlet.RequestTestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tset</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
</web-app>